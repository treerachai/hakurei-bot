{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["lib", "README.md"]
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "private": true
    },
    "templates": {
        "monospaceLinks": true
    },
    "plugins": ["plugins/markdown"]
}
